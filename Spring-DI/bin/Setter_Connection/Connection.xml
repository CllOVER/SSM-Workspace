<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:aop="http://www.springframework.org/schema/aop"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.2.xsd
           http://www.springframework.org/schema/aop
           http://www.springframework.org/schema/aop/spring-aop-3.2.xsd">
           
       <!-- Connection集合注入 -->
       <bean id="connection" class="Setter_Connection.Connection">
       
      <property name="listValue">
      <list>
      <value>listValue1</value>
      <value>listValue2</value>
      </list>
      </property>	
      
      <property name="setValue">
      <set>
      <value>setValue1</value>
      <value>setValue2</value>
      </set>
      </property>
      
      <property name="ArrayValue">
      <array>
      <value>arrayValue1</value>
      <value>arrayValue2</value>
      </array>
      </property>
      
      <property name="mapValue">
      <map>
      <entry key="mapkey1" value="mapvalue1"></entry>
      <entry key="mapkey2" value="mapvalue2"></entry>
      </map>
      </property>
      
      <property name="propValue">
      <props>
      <prop key="key1">value1</prop>
      <prop key="key2">value2</prop>
      </props>
      </property>
      
       </bean>
       
</beans>